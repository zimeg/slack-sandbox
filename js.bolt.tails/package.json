  "license": "MIT",
  "dependencies": {
    "@slack/bolt": "file:../../tools/bolt-js",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
