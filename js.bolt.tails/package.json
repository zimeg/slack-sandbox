{
  "name": "tails",
  "version": "0.0.1",
  "description": "a Slack app that does various things",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "npx nodemon --watch 'src' --ext 'ts' --exec npm run build",
    "dev": "npx nodemon --watch 'dist' --exec node dist/app.js",
    "http": "ngrok http 3000",
    "refresh": "rm -rf node_modules && npm install --save",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
